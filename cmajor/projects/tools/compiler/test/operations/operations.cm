using System;

void BoolOp()
{
    bool x = true;
    bool y = !x;
}

void SByteOp()
{
    sbyte x = 0;
    sbyte y = 1;
    sbyte z = 2;
    x = y + z;
    x = y - z;
    x = y * z;
    x = y / z;
    x = y % z;
    x = y << z;
    x = y >> z;
    x = y & z;
    x = y | z;
    x = y ^ z;
    x = -y;
    x = ~y;
}

