using System;
using System.Collections;

unit_test void int_stack_test()
{
    Stack<int> s;
    #assert(s.IsEmpty());
    #assert(s.Count() == 0);
    s.Push(1);
    #assert(!s.IsEmpty());
    #assert(s.Count() == 1);
    #assert(s.Top() == 1);
    auto v0 = s.Pop();
    #assert(v0 == 1);
    #assert(s.IsEmpty());
    #assert(s.Count() == 0);

    s.Push(1);
    s.Push(2);
    #assert(!s.IsEmpty());
    #assert(s.Count() == 2);
    #assert(s.Top() == 2);
    auto v1 = s.Pop();
    #assert(v1 == 2);
    #assert(!s.IsEmpty());
    #assert(s.Count() == 1);
    auto v2 = s.Pop();
    #assert(v2 == 1);
    #assert(s.IsEmpty());
    #assert(s.Count() == 0);

    for (int i = 0; i < 8; ++i)
    {
        s.Push(i);
    }
    #assert(!s.IsEmpty());
    #assert(s.Count() == 8);
    for (int i = 0; i < 8; ++i)
    {
        auto v = s.Pop();
        #assert(v == 7 - i);
        if (i < 7)
        {
            #assert(!s.IsEmpty());
            #assert(s.Count() == 7 - i);
        }
        else
        {
            #assert(s.IsEmpty());
            #assert(s.Count() == 0);
        }
    }
}
