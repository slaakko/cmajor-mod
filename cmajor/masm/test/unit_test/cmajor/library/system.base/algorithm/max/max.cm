using System;

class X
{
    public X(int id_) : id(id_) {}
    public int id;
}

inline bool operator==(const X& left, const X& right)
{
    return left.id == right.id;
}

inline bool operator<(const X& left, const X& right)
{
    return left.id < right.id;
}

unit_test void max_test()
{
    #assert(Max(0, 1) == 1);
    #assert(Max(1, 0) == 1);
    #assert(Max(0, 0) == 0);
    #assert(Max(-1, 0) == 0);
    #assert(Max(0, -1) == 0);
    #assert(Max(-1, -1) == -1);
    #assert(Max(1, 1) == 1);
    auto x0 = X(0);
    auto x1 = X(1);
    #assert(Max(x0, x0) == x0);
    #assert(Max(x0, x1) == x1);
    #assert(Max(x1, x0) == x1);
    auto x2 = X(0);
    #assert(Max(x0, x2) == x2);
    #assert(Max(x2, x0) == x0);
    string s0 = "0";
    string s1 = "1";
    string s2 = "0";
    #assert(Max(s0, s1) == s1);
    #assert(Max(s1, s0) == s1);
    #assert(Max(s0, s2) == s2);
    #assert(Max(s2, s0) == s0);
}
