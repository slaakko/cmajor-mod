using System;
using System.Collections;

class X
{
    public X(int id_) : id(id_) {}
    public int id;
}

unit_test void move_test()
{
    List<UniquePtr<X>> l;
    for (int i = 0; i < 8; ++i)
    {
        l.Add(UniquePtr<X>(new X(i)));
    }
    Move(l.Begin() + 4, l.End(), l.Begin());
    for (int i = 0; i < 4; ++i)
    {
        #assert(l[i]->id == i + 4);
    }
}
