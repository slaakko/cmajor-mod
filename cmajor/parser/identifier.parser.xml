<parserFile>
 <grammar name="IdentifierParser">
  <rule first="{ID}" name="Identifier">
   <action first="{ID}">
    <token first="{ID}" name="ID"/>
   </action>
  </rule>
  <rule first="{ID}" name="QualifiedId">
   <action first="{ID}">
    <group first="{ID}">
     <sequence first="{ID}">
      <action first="{ID}">
       <token first="{ID}" name="ID"/>
      </action>
      <kleene first="{#, DOT}">
       <group first="{DOT}">
        <sequence first="{DOT}">
         <token first="{DOT}" name="DOT"/>
         <action first="{ID}">
          <token first="{ID}" name="ID"/>
         </action>
        </sequence>
       </group>
      </kleene>
     </sequence>
    </group>
   </action>
  </rule>
 </grammar>
</parserFile>
